#include <bits/stdc++.h>
using namespace std;

class linkedlist{
private:
    Node* head;
    public:
    linkedlist(){
        head = NULL;
    }
    void insert(int n){
        Node* temp = new Node();
        temp->data = n;
        temp->next = head;
        head = temp;
    }
    void display(){
        Node* temp = head;
        while(temp != NULL){
            cout << temp->data << " ";
            temp = temp->next;
        }
    }
};




